<?xml version="1.0" encoding="utf-8"?>
<configuration>
 
 <configSections>
  <sectionGroup name="variableConfig">
   <section name="variableSection"   type="System.Configuration.NameValueSectionHandler" />
  </sectionGroup> 
  <sectionGroup name="taskListConfig">
   <section name="taskConfiguration" type="ConfigHandler.CustomSectionHandler, ConfigHandler" />
  </sectionGroup> 
  <sectionGroup name="addressConfig">
   <section name="addressConfiguration" type="ConfigHandler.AddressSectionHandler, ConfigHandler" />
  </sectionGroup> 
  <sectionGroup name="addressXMLConfig">
   <section name="addressXMLConfiguration" type="ConfigHandler.XMLSectionHandler, ConfigHandler" />
  </sectionGroup> 
 </configSections>

 <variableConfig>
  <variableSection> 
   <add key="v1" value="val1"/>
   <add key="v2" value="val2"/>
  </variableSection>
 </variableConfig> 
 
 <taskListConfig>
  <taskConfiguration> 
   <task value="start_1" time="10" />
   <task value="start_2" time="20" />
   <task value="start_3" time="30" />
  </taskConfiguration>
 </taskListConfig> 

 <addressConfig>
  <addressConfiguration> 
    <company>PVASoft</company>
    <country>Russia</country>
  </addressConfiguration> 
 </addressConfig> 

 <addressXMLConfig>
  <addressXMLConfiguration> 
    <company>PVASoft1</company>
    <country>Russia1</country>
  </addressXMLConfiguration> 
 </addressXMLConfig> 

</configuration>


